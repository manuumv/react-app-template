    "start": "npm run start:dev",
    "start:dev": "webpack-dev-server --mode development --inline --config ./config/webpack/webpack.dev.js",
    "start:prod": "npm run build:prod && node ./server",
    "build:dev": "rimraf dist && webpack --config ./config/webpack/webpack.dev.js",
